			int wt = edges[i][0];
			int src = edges[i][1];
			int dest = edges[i][2];
			ArrayList<Integer> temp1 = new ArrayList<Integer>();
			ArrayList<Integer> temp1 = new ArrayList<Integer>();
			
			
		}
		
	}
}
