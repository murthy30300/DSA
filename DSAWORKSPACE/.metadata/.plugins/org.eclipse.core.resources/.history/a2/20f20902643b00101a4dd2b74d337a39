package com.disjointset;
import java.util.*;
class Edge implements Comparable<Edge>{

	int src, wt, dest;
	public Edge(int src, int wt, int dest) {
		this.src = src;
		this.wt = wt;
		this.dest = dest;
	}
	public int compareTo(Edge edge) {
		return this.wt - edge.wt;
	}
}
