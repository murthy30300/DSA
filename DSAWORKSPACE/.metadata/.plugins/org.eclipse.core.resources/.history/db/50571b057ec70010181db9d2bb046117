 package com.tcs.roundii;   // <-- keep or remove depending on your environment

import java.io.*;
import java.util.*;

public class Cubemid {

    static class Cell {
        int l, r, c;
        Cell(int l, int r, int c) {
            this.l = l;
            this.r = r;   // âœ… FIXED
            this.c = c;
        }
    }

    // Walkable = anything except 'E'
    static boolean isWalkable(char x) {
        return x != 'E';
    }

    public static void main(String[] args) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        String line = br.readLine();
        if (line == null || line.trim().isEmpty()) {
            System.out.println(-1);
            return;
        }

        int S = Integer.parseInt(line.trim());

        char[][][] cube = new char[S][S][S];
        for (int l = 0; l < S; l++) {
            for (int r = 0; r < S; r++) {
                String row = br.readLine();
                while (row != null && row.trim().isEmpty()) {
                    row = br.readLine(); // skip blank lines if any
                }
                row = row.trim();
                for (int c = 0; c < S; c++) {
                    cube[l][r][c] = row.charAt(c);
                }
            }
        }

        String[] s = br.readLine().trim().split("\\s+");
        int sl = Integer.parseInt(s[0]);
        int sr = Integer.parseInt(s[1]);
        int sc = Integer.parseInt(s[2]);

        String[] g = br.readLine().trim().split("\\s+");
        int gl = Integer.parseInt(g[0]);
        int gr = Integer.parseInt(g[1]);
        int gc = Integer.parseInt(g[2]);

        // If start or goal itself is blocked
        if (!isWalkable(cube[sl][sr][sc]) || !isWalkable(cube[gl][gr][gc])) {
            System.out.println(-1);
            return;
        }

        boolean[][][] vis = new boolean[S][S][S];
        int[][][] dist = new int[S][S][S];

        for (int i = 0; i < S; i++) {
            for (int j = 0; j < S; j++) {
                Arrays.fill(dist[i][j], -1);
            }
        }

        Queue<Cell> q = new ArrayDeque<>();
        q.add(new Cell(sl, sr, sc));
        vis[sl][sr][sc] = true;
        dist[sl][sr][sc] = 0;

        // 4 directions in the same layer (row/column moves)
        int[][] dirs = { { -1, 0 }, { 1, 0 }, { 0, -1 }, { 0, 1 } };

        while (!q.isEmpty()) {
            Cell cur = q.poll();
            int l = cur.l, r = cur.r, c = cur.c;

            // Reached golden chamber
            if (l == gl && r == gr && c == gc) {
                System.out.println(dist[l][r][c]);
                return;
            }

            // Same-layer moves
            for (int[] dir : dirs) {
                int nr = r + dir[0];
                int nc = c + dir[1];
                if (nr >= 0 && nr < S && nc >= 0 && nc < S) {
                    if (!vis[l][nr][nc] && isWalkable(cube[l][nr][nc])) {
                        vis[l][nr][nc] = true;
                        dist[l][nr][nc] = dist[l][r][c] + 1;
                        q.add(new Cell(l, nr, nc));
                    }
                }
            }

            // Special moves based on current chamber type
            char type = cube[l][r][c];

            switch (type) {
                case 'R':
                    // Teleport to (layer+1, same row, column+1)
                    if (l + 1 < S && c + 1 < S &&
                        !vis[l + 1][r][c + 1] &&
                        isWalkable(cube[l + 1][r][c + 1])) {

                        vis[l + 1][r][c + 1] = true;
                        dist[l + 1][r][c + 1] = dist[l][r][c] + 1;
                        q.add(new Cell(l + 1, r, c + 1));
                    }
                    break;

                case 'L':
                    // Teleport to (layer+1, same row, column-1)
                    if (l + 1 < S && c - 1 >= 0 &&
                        !vis[l + 1][r][c - 1] &&
                        isWalkable(cube[l + 1][r][c - 1])) {

                        vis[l + 1][r][c - 1] = true;
                        dist[l + 1][r][c - 1] = dist[l][r][c] + 1;
                        q.add(new Cell(l + 1, r, c - 1));
                    }
                    break;

                case 'F':
                    // Teleport to (layer+1, same row, same column)
                    if (l + 1 < S &&
                        !vis[l + 1][r][c] &&
                        isWalkable(cube[l + 1][r][c])) {

                        vis[l + 1][r][c] = true;
                        dist[l + 1][r][c] = dist[l][r][c] + 1;
                        q.add(new Cell(l + 1, r, c));
                    }
                    break;

                case 'B':
                    // Teleport to (layer-1, same row, same column)
                    if (l - 1 >= 0 &&
                        !vis[l - 1][r][c] &&
                        isWalkable(cube[l - 1][r][c])) {

                        vis[l - 1][r][c] = true;
                        dist[l - 1][r][c] = dist[l][r][c] + 1;
                        q.add(new Cell(l - 1, r, c));
                    }
                    break;

                case 'D':
                    // Vertical movement both ways
                    if (l + 1 < S &&
                        !vis[l + 1][r][c] &&
                        isWalkable(cube[l + 1][r][c])) {

                        vis[l + 1][r][c] = true;
                        dist[l + 1][r][c] = dist[l][r][c] + 1;
                        q.add(new Cell(l + 1, r, c));
                    }
                    if (l - 1 >= 0 &&
                        !vis[l - 1][r][c] &&
                        isWalkable(cube[l - 1][r][c])) {

                        vis[l - 1][r][c] = true;
                        dist[l - 1][r][c] = dist[l][r][c] + 1;
                        q.add(new Cell(l - 1, r, c));
                    }
                    break;
            }
        }

        // No path
        System.out.println(-1);
    }
}
